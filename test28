//преобразуйте обьект ниже в конструктор функции
let obj = {
    name: 'Alex',
    age: 24,
    isMarried: false,
    hasCar: false,
    hasJob: true,
    job: 'Programmer',
    childs: ['Hanz', 'Marry'],
    awards: {
        itMaster: 'Legend rang',
        driver: 'Fast man'
    }
}


function obj(name,age,ismarried,hascar,hasob,job,childs,awards) = {
    this.name = name
    this.age  = age
    this.isMarried = isMarried
    this.hasCar: hasCar
    this.hasJob: hasJob
    this.job: job
    this.childs = childs
    this.awards = awards
}


