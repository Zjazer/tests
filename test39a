18)
//удалите job из обьекта и поменяйте значение hasJob на 'false'
let obj = {
    name: 'Alex',
    age: 24,
    isMarried: false,
    hasCar: false,
    hasJob: true,
    job: 'Programmer',
    childs: ['Hanz', 'Marry'],
    awards: {
        itMaster: 'Legend rang',
        driver: 'Fast man'
    }
}


let x = delete(obj.job)
obj.hasJob = false
console.log(obj);

