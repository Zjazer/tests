let text = 'My name is Alexander';
let a = text.toUpperCase()
console.log(a);

2)
//превратите строку в строку в нижнем регистре 'ABC' -> 'abc'
let text = 'MY NAME IS ALEXANDER';

let text = 'My name is Alexander';
let a = text.toLowerCase()
console.log(a);

