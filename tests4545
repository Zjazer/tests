5)
//преобразуйте строки ниже в числа c помощью Number
let a = '23.5';
let b = '20';
let d = '0000000.1';
let c = ' ';
let n = 'hello';

let a = Number('23.5')
let b = Number('20')
let d = Number('0000000.1')
let c = Number(' ')
let n = Number('hello')

console.log(a);
console.log(b);
console.log(d);
console.log(c);
console.log(n);

