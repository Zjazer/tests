7)
//вырежте из строк ниже целую часть числа и выведите в консоли
let a = '23.5px';
let b = '20rem';
let d = '0000000.1deg';

let a = parseInt ('23.5px')
let b = parseInt ('20rem')
let d = parseInt( '0000000.1deg')

console.log(a);
console.log(b);
console.log(d);


8)
//вырежте из строк ниже числа вместе с плавающей точкой и выведите в консоли
let a = '23.5px';
let b = '20rem';
let d = '0000000.1deg';

let a = parseFloat('23.5px')
let b = parseFloat('20rem')
let d = parseFloat('0000000.1deg')

console.log(a);
console.log(b);
console.log(d);


9)
//преобразуйте переменные ниже в строки с помощью String
let a = 234;
let b = true;

let a = String(234)
let b = String(true)

console.log(a);
console.log(b);

10)
//преобразуйте переменные ниже в строки с помощью toString
let a = 234;
let b = true;

let a = (234).toString()
let b = (true).toString()

console.log(a);
console.log(b);

